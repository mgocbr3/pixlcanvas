{
  "name": "pixlland-api",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Pixlland backend API for PlayCanvas-based editor",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "ws:dev": "node scripts/ws-servers.mjs",
    "editor:dev": "node scripts/editor-proxy.mjs",
    "editor:full": "concurrently -n backend,proxy,ws -c cyan,magenta,yellow \"npm run dev\" \"npm run editor:dev\" \"npm run ws:dev\"",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/http-proxy": "^10.0.2",
    "@fastify/jwt": "^9.0.0",
    "@fastify/multipart": "^9.0.0",
    "@fastify/static": "^8.1.0",
    "@supabase/supabase-js": "^2.49.1",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.0",
    "sharedb": "^1.9.2",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "concurrently": "^9.2.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
